_______________________________________________layer 3 sw1_______________________________________________
enable
conf t
hostname HQ_sw1
enable secret cisco

service password-encryption
banner motd ^
******************************************
UNATHOURIZED ACCESS IS BROHIBITED
******************************************
^

line con 0
pass cisco
login local
logging synchro
no exec-timeout 
exit

ip domain-name admin.com
username admin secret cisco
crypto key generate rsa general-keys modulus 2024
ip ssh version 2

line vty 0 15
login local
logging synchro
no exec-timeout 
transport input ssh
exit

vlan 10
name first_floor
vlan 20
name second_floor
vlan 30
name third_floor
vlan 40
name voice_vlan
exit

int vlan 10
no sh
ip helper-address 192.168.4.2
ip address 192.168.0.1 255.255.255.192
exit

int vlan 20
no sh
ip helper-address 192.168.4.2
ip address 192.168.0.65 255.255.255.192
exit

int vlan 30
no sh
ip helper-address 192.168.4.2
ip address 192.168.0.129 255.255.255.192
exit

int vlan 40
no sh
ip helper-address 192.168.4.2
ip address 192.168.0.193 255.255.255.192
exit

spanning-tree mode rapid-pvst 
spanning-tree vlan 10 root primary 
spanning-tree vlan 20 root primary 
spanning-tree vlan 30 root primary 
spanning-tree vlan 40 root primary 

int r fa0/4-6
channel-group 1 mode active
channel-protocol lacp

int fa0/7
no switchport
ip address 192.168.3.2 255.255.255.252

ip routing
router ospf 1
router-id 2.2.2.2
network 192.168.0.0 0.0.0.63 a 0
network 192.168.0.64 0.0.0.63 a 0
network 192.168.0.128 0.0.0.63 a 0
network 192.168.0.192 0.0.0.63 a 0
network 192.168.3.0 0.0.0.3 a 0
