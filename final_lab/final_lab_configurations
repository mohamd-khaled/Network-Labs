no exec-timeout 
exit

line vty 0 15
logging synchro
no exec-timeout 
exit

vlan 20
name second_floor
exit

int r fa3/1, fa6/1
switchport mode trunk
exit

int r fa0/1, fa1/1, fa2/1,
switchport mode access
switchport access vlan 20
exit


